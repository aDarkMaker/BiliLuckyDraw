        "@vitejs/plugin-vue": "^3.0.3",
        "cspell": "^9.4.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-vue": "^10.6.2",
        "prettier": "^3.7.4",
        "typescript": "^4.6.4",
        "typescript-eslint": "^8.50.0",
        "vite": "^3.0.7",
        "vue-tsc": "^1.8.27"
    }
